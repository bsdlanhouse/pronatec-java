// tip: each public class is put in its own file
public class main
{
    // tip: arguments are passed via the field below this editor
    public static void main(String[] args)
    {
        boolean var_boolean = false;
        int idade = 20;
        float peso = 90.7f;
        double altura = 1.90;
        char letra ='c';
        String name = "João das Neves";

        System.out.println("O conteúdo da variável boolean é:" +var_boolean);
         System.out.println("O conteúdo da variável inteiro é:" +idade);
           System.out.println("O conteúdo da flutuante é:" +peso);
             System.out.println("O conteúdo da dobro é:" +altura);
              System.out.println("O conteúdo da caracter é:" +letra);
               System.out.println("O conteúdo da String é:" +name);
             } 

}
